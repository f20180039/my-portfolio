{
  "name": "my-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && tsc && vite build",
    "build:production": "npm run lint && tsc && vite build --mode production",
    "build:qa": "npm run lint && tsc && vite build --mode qa",
    "build:testprod": "npm run lint && tsc && vite build --mode testprod",
    "lint:fix": "eslint ./src --ext .ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "eslint --fix",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "eslint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dayjs": "^1.11.12",
    "dompurify": "^3.2.1",
    "html2pdf.js": "^0.9.3",
    "js-cookie": "^3.0.5",
    "jspdf": "^2.5.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-pdf": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "11.0.2",
    "swr": "^2.2.5",
    "uuid": "^11.0.3",
    "zustand": "^4.5.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "devDependencies": {
    "@types/jquery": "^3.5.29",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
